{
	"servers": {
		"dk-math": {
			"command": "~/.local/bin/uv",
			"args": [
				"--directory",
				"~/Documents/dev/workspaces/python/dk_calc",
				"run",
				"dk_math.py"
			]
		},
		"my-customer-java": {
			"command": "java",
			"args": [
				"-jar",
				"~/Documents/dev/workspaces/dis-demo/mcp-customer-service/target/mcp-customer-service-0.0.1-SNAPSHOT.jar"
			]
		},
		"postgres-full": {
			"command": "npx",
			"args": [
				"-y",
				"mcp-postgres-full-access",
				"postgresql://postgres:postgres@localhost:5432/postgres"
			],
			"env": {
				"TRANSACTION_TIMEOUT_MS": "60000",
				"MAX_CONCURRENT_TRANSACTIONS": "5",
				"PG_STATEMENT_TIMEOUT_MS": "30000"
			}
		}
	}
}